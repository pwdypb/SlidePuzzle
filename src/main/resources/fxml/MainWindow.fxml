      minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
      fx:controller="hu.kampeni.controller.MainWindowController">
    <children>
        <Button layoutX="274.0" layoutY="106.0" mnemonicParsing="false" onAction="#handleNewGame"
                fx:id="newGameButton"/>
        <Button layoutX="270.0" layoutY="188.0" mnemonicParsing="false" onMouseClicked="#handleLoadGame"
                fx:id="loadGameButton"/>
        <Button layoutX="274.0" layoutY="250.0" mnemonicParsing="false" onAction="#handleExitGame"
                fx:id="exitGameButton"/>
    </children>
</Pane>
